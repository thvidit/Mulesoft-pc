    <munit:config name="munit" doc:name="MUnit configuration"/>
    <spring:beans>
        <spring:import resource="classpath:javamessagingservice.xml"/>
    </spring:beans>
    <munit:test name="javamessagingservice-test-suite-JMS-ProducerTest" description="Test">
        <dw:transform-message doc:name="Transform Message">
            <dw:set-payload><![CDATA[%dw 1.0
%output application/json
---
{
	"message": "hi"
}]]></dw:set-payload>
        </dw:transform-message>
        <flow-ref name="JMS-Producer" doc:name="Flow-ref to JMS-Producer"/>
        <logger message="Payload is #[payload]" level="INFO" doc:name="Logger"/>
        <munit:assert-payload-equals expectedValue="{&quot;message&quot;: &quot;Successfully sent message!&quot;} " doc:name="Assert Payload"/>
    </munit:test>
    <munit:test name="javamessagingservice-test-suite-JMS-ConsumerTest" description="Test">
        <flow-ref name="JMS-Consumer" doc:name="Flow-ref to JMS-Consumer"/>
    </munit:test>
